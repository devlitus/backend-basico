{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["sockets", "controllers", "helpers", "middleware"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opt": {
        "recurse": "true",
        "destination": "./docs"
    }
}